{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "version": "6.0.9",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-language-switch": "^3.1",
        "endroid/qr-code": "^5.0",
        "filament/filament": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "firebase/php-jwt": "^6.10",
        "gerardojbaez/money": "^0.3.1",
        "guzzlehttp/guzzle": "^7.9",
        "hasnayeen/themes": "^3.0",
        "lab404/laravel-impersonate": "^1.7",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^12.15",
        "laravel/nightwatch": "^1.7",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "league/flysystem-aws-s3-v3": "~3.0",
        "livewire/livewire": "^3.5",
        "lorisleiva/laravel-actions": "^2.9",
        "malzariey/filament-daterangepicker-filter": "^3.0",
        "mariuzzo/laravel-js-localization": "^2.0",
        "opcodesio/log-viewer": "^3.15",
        "outhebox/laravel-translations": "^1.3",
        "owenvoke/blade-fontawesome": "^2.9",
        "pxlrbt/filament-excel": "^2.3",
        "razorpay/razorpay": "^2.9",
        "saade/filament-fullcalendar": "^3.2",
        "spatie/laravel-google-calendar": "^3.8",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-permission": "^6.12",
        "srmklive/paypal": "^3.0",
        "stancl/tenancy": "^3.9",
        "stechstudio/filament-impersonate": "^3.14",
        "stevebauman/location": "*",
        "stripe/stripe-php": "^16.4",
        "tapp/filament-country-code-field": "^1.0",
        "tightenco/ziggy": "^2.4",
        "twilio/sdk": "^8.3",
        "vormkracht10/filament-2fa": "^2.1",
        "ysfkaya/filament-phone-input": "^2.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
